{
  "version": "1.1.0",
  "keywords": [
    "strapi",
    "plugin",
    "slug",
    "translation",
    "pinyin",
    "seo",
    "url",
    "baidu",
    "tencent",
    "deepl",
    "google-translate",
    "alibaba"
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@alicloud/alimt20181012": "^1.4.1",
    "@alicloud/credentials": "^2.4.4",
    "@alicloud/openapi-core": "^1.0.5",
    "@darabonba/typescript": "^1.0.3",
    "@strapi/design-system": "^2.0.1",
    "@strapi/icons": "^2.0.1",
    "@vitalets/google-translate-api": "^9.2.1",
    "@volcengine/openapi": "^1.32.0",
    "axios": "^1.13.2",
    "baidu-translate-api": "^0.4.3",
    "crypto-js": "^4.2.0",
    "deepl-node": "^1.20.0",
    "pinyin-pro": "^3.27.0",
    "react-intl": "^7.1.14",
    "tencentcloud-sdk-nodejs-tmt": "^4.1.134"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.31.2",
    "@strapi/typescript-utils": "^5.31.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.31.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "enhanced-slug",
    "displayName": "enhanced-slug",
    "description": "strapi enhanced slug"
  },
  "publishConfig": {
    "access": "public"
  },
  "name": "enhanced-slug",
  "description": "strapi enhanced slug",
  "license": "MIT",
  "author": "zhongjingjing <zhongjingjing@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/xuhuan/enhanced-slug.git"
  },
  "bugs": {
    "url": "https://github.com/xuhuan/enhanced-slug/issues"
  },
  "homepage": "https://github.com/xuhuan/enhanced-slug",
  "funding": "https://github.com/xuhuan/enhanced-slug?tab=readme-ov-file#%E8%81%94%E7%B3%BB%E6%94%AF%E6%8C%81%E4%BD%9C%E8%80%85"
